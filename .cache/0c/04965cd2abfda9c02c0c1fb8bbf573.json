rem;\r\n    height: 5rem;\r\n}\r\n\r\n.menu {\r\n    display: flex;\r\n    flex-direction: column;\r\n    position: fixed;\r\n    flex-wrap: wrap;\r\n    height: fit-content;\r\n}\r\n\r\n.menu__icon {\r\n    position: static;\r\n    margin: 1rem;\r\n    fill: var(--color-dark);\r\n}\r\n\r\n.menu__list {\r\n    background: var(--color-blue);\r\n    opacity: .5;\r\n    list-style: none;\r\n    padding-left: 0;\r\n    margin: 0 0 1rem 1rem;\r\n    border-radius: 5px;\r\n    color: white;\r\n}\r\n\r\n.menu__item {\r\n    padding: 1rem 3rem;\r\n    font-size: 3rem;\r\n}\r\n\r\n.collapsible__content {\r\n    max-height: 0;\r\n    overflow: hidden;\r\n    opacity: 0;\r\n    transition: all 0.3s;\r\n}\r\n\r\n.collapsible--expanded .collapsible__content {\r\n    max-height: 100vh;\r\n    opacity: 1;\r\n}\r\n\r\n.collapsible--expanded .menu__icon {\r\n    fill: var(--color-blue);\r\n}\r\n\r\n.to-left {\r\n    rotate: 180deg;\r\n}\r\n\r\n.to-right {\r\n    right: 0;\r\n}\r\n\r\n.to-right, .to-left {\r\n    top: 50%;\r\n    transition: transform 0.3s;\r\n    opacity: 0.70;\r\n}\r\n\r\n.to-left:active, .to-right:active {\r\n    transform: scale(1.5);\r\n}\r\n\r\n.full_pic {\r\n    width: 100vw;\r\n}\r\n\r\n.ahmola {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    position: fixed;\r\n    height: 6rem;\r\n    max-width: 90%;\r\n    min-width: 25rem;\r\n}\r\n\r\n.ahmola1 {\r\n    top: 42%;\r\n    left: 21%;\r\n}\r\n\r\n.ahmola2 {\r\n    top: 32%;\r\n    left: 10%;\r\n}\r\n\r\n.mg {\r\n    fill: white;\r\n    filter: drop-shadow(0px 0px 4px grey);\r\n    opacity: 0.85;\r\n}\r\n\r\n.mg--clicked {\r\n    display: none;\r\n}\r\n\r\n.mg--clicked + .input {\r\n    display: block;\r\n}\r\n\r\n.input {\r\n    display: none;\r\n}\r\n\r\n.input--correct .clue, .input--correct .btn{\r\n    display: none;\r\n}\r\n\r\n.clue {\r\n    border: 1px solid var(--color-dark);\r\n    outline: 0;\r\n    border-radius: 5px;\r\n    padding: 1rem 0;\r\n    max-width: fit-content;\r\n    color: var(--color-blue);\r\n    text-align: center;\r\n}\r\n\r\n.btn {\r\n    background: var(--color-blue);\r\n    color: white;\r\n    border: 0;\r\n    border-radius: 5px;\r\n    padding: 1rem;\r\n    height: 100%;\r\n}\r\n\r\npicture, .pic {\r\n    align-self: center;\r\n}\r\n\r\n\r\n.click{\r\n    position: fixed;\r\n    left: 0;\r\n    align-self: center;\r\n    overflow: hidden;\r\n}\r\n\r\n.click__detective {\r\n    clip-path: ellipse(24% 20% at 53% 49%);\r\n}\r\n\r\n.click__chest {\r\n    clip-path: ellipse(14% 7% at 16% 65%);\r\n}\r\n\r\n.click__map {\r\n    /* background: var(--color-su); */\r\n    clip-path: ellipse(16% 11% at 14% 36%);\r\n}\r\n\r\n.evidence {\r\n    filter: drop-shadow(0px 0px 2px var(--color-dark));\r\n    transition: transform 0.1s;\r\n}\r\n\r\n.evidence:active {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.bag__item{\r\n    left: 0;\r\n    position: fixed;\r\n    overflow: hidden;\r\n    align-self: center;\r\n}\r\n\r\n.item_desc {\r\n    background: var(--color-dark);\r\n    color: var(--color-base);\r\n    position: absolute;\r\n    bottom: 5%;\r\n    padding: 1rem;\r\n    border-radius: 2rem;\r\n    text-align: center;\r\n    line-height: 1.4;\r\n}\r\n\r\n\r\n\r\n/* bottom */\r\n\r\n.bottom {\r\n    display: flex;\r\n    padding: 0 10px 10px 10px;\r\n    position: relative;\r\n}\r\n\r\n.frog, .bunny {\r\n    top: -10%;\r\n}\r\n\r\n.frog {\r\n    right: 85%;\r\n}\r\n\r\n.bunny {\r\n    left: 85%;\r\n}\r\n\r\n.box {\r\n    display: flex;\r\n    background: var(--color-base);\r\n    border: 5px dotted #b6a989;\r\n    overflow: scroll;\r\n    border-radius: 3rem;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.text {\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding: 0.5rem;\r\n    text-align: center;\r\n    color: var(--color-dark);\r\n    line-height: 1.4;    \r\n}\r\n\r\n.clue__text, .clue__text * {\r\n    max-width: 0;\r\n    margin: 0;\r\n    overflow: scroll;\r\n}\r\n\r\n.bag1 {\r\n    display: grid;\r\n    margin: 0;\r\n    padding: 1rem;\r\n    grid-gap: 1rem;\r\n    grid-template-columns: repeat(2, 1fr);\r\n}\r\n\r\n.item {\r\n    display: flex;\r\n    margin: 0;\r\n    padding: 0;\r\n    border-radius: 2rem;\r\n    overflow: hidden;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.item:nth-child(1),\r\n.item:nth-child(3),\r\n.item:nth-child(6),\r\n.item:nth-child(8) {\r\n    background: rgb(56, 167, 0, 0.5);\r\n}\r\n\r\n.item:nth-child(2),\r\n.item:nth-child(4),\r\n.item:nth-child(5),\r\n.item:nth-child(7) {\r\n    background: rgb(255, 130, 169, 0.5);\r\n}\r\n\r\n.collected__item, .collected__item * {\r\n    width: 100%;\r\n    opacity: 0.5;\r\n}\r\n\r\n.selected__item, .selected__item *{\r\n    opacity: 1;\r\n}"},"lineCount":null}},"error":null,"hash":"9ebd4a5901312457805bf42830bba2a8","cacheData":{"env":{}}}